{
    "name": "matheusmariano/tumblr",
    "description": "A unofficial Tumblr API Client for PHP.",
    "keywords": ["tumblr", "api", "client"],
    "homepage": "https://github.com/matheusmariano/tumblr-php-client",
    "authors": [
        {
            "name": "Matheus Mariano",
            "email": "vmatheus.mariano@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/oauth-subscriber": "0.3.*",
        "illuminate/support": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Niobium\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niobium\\Tests\\": "tests/"
        }
    }
}
